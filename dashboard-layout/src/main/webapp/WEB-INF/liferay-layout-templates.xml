<?xml version="1.0"?>
<!DOCTYPE layout-templates PUBLIC "-//Liferay//DTD Layout Templates 6.2.0//EN" "http://www.liferay.com/dtd/liferay-layout-templates_6_2_0.dtd">

<layout-templates>
	<custom>
		<layout-template id="dashboard-layout"
			name="dashboard-layout">
			<template-path>/dashboard-layout.tpl</template-path>
			<wap-template-path>
				/dashboard-layout.wap.tpl
			</wap-template-path>
			<thumbnail-path>/dashboard-layout.png</thumbnail-path>
		</layout-template>
		<layout-template id="dashboard" name="Dashboard">
			<template-path>/dashboard.tpl</template-path>
			<wap-template-path>/dashboard.wap.tpl</wap-template-path>
			<thumbnail-path>/dashboard.png</thumbnail-path>
		</layout-template>
	</custom>
</layout-templates>