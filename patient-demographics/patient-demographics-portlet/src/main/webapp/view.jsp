<%@ taglib uri="http://java.sun.com/portlet_2_0" prefix="portlet"%>

<portlet:defineObjects />

<div class="portal-header-content">Patient Demographics</div>

<div class="portal-inner-content">

	<div class="container">

		<div class="row-fluid">
			<div class="span5 primary-column-2">Gender</div>
		</div>

		<div class="row-fluid">
			<div class="span3 primary-column no-style">Male</div>
			<div class="span2 secondary-column">50%</div>
		</div>

		<div class="row-fluid">
			<div class="span3 primary-column no-style">Female</div>
			<div class="span2 secondary-column">50%</div>
		</div>
	</div>

</div>

<div class="portal-inner-content margin-top">

	<div class="container">

		<div class="row-fluid">
			<div class="span5 primary-column-2">Insurance Type</div>
		</div>

		<div class="row-fluid">
			<div class="span3 primary-column no-style">Medicare</div>
			<div class="span2 secondary-column">50</div>
		</div>

		<div class="row-fluid">
			<div class="span3 primary-column no-style">Medicade</div>
			<div class="span2 secondary-column">30</div>
		</div>

		<div class="row-fluid">
			<div class="span3 primary-column no-style">Group Commercial</div>
			<div class="span2 secondary-column">18%</div>
		</div>

		<div class="row-fluid">
			<div class="span3 primary-column no-style">Ind. Commercial</div>
			<div class="span2 secondary-column">0%</div>
		</div>

		<div class="row-fluid">
			<div class="span3 primary-column no-style">Cash Customer</div>
			<div class="span2 secondary-column">2%</div>
		</div>

	</div>

</div>


